{
    "NTOR":    1,      // Toroidal mode number
    "MMIN":   -12,     // Minimum poloidal mode number included in calculation
    "MMAX":   20,      // Maximum poloidal mode number included in calculation
    "FREE":   1,       // Flag for free boundary calculation
    "EQLB":   0,       // Flag for equilibrium calculation only
    "XiFlag": 1,       // Flag for using Xi, rather than Psi, as ideal eigenfunction basis

    "Rcoil": [1.10,  1.125,  1.125,  1.10],  // R coodinates of RMP coil strands
    "Zcoil": [0.1,   0.05,  -0.05,  -0.1],   // Z coodinates of RMP coil strands
    "Icoil": [1.0,  -1.0,    1.0,   -1.0],   // Toroidal currents flowing in RMP coil strands

    "B0":    12,           // On-axis toroidal magnetic field-strength (T)
    "R0":    2.0,          // On-axis plasma major radius (m) 
    "n0":    3.0e20,       // On-axis electron number density (m^-3)
    "alpha": 0.5,          // Assumed electron number density profile: n0 (1 - r^2)^alpha
    "Zeff":  2.0,          // Effective ion charge number
    "Mion":  2.0,          // Ion mass number
    "Chip":  0.2,          // Perpendicular momentum/energy diffusivity (m^2/s)
    "Teped": 100.0,        // Electron temperature at edge of plasma (eV)
    
    "EPS":   1.0e-4,       // Solutions launched from magnetic axis at r = EPS 
    "DEL":   1.0e-9,       // Distance of closest approach to rational surface
    "NFIX":  160,          // Number of fixups 
    "NDIAG": 1024,         // Number of radial grid-points for diagnostics
    
    "NULC":    2.0e-9,     // Use zero pressure jump conditions when |nu_L| < NULC 
    "ITERMAX": 10,         // Maximum number of iterations used to determine quantities at rational surface 
    
    "acc":  1.0e-12,       // Integration accuracy
    "h0":   1.0e-4,        // Initial step-length 
    "hmin": 1.0e-10,       // Minimum step-length 
    "hmax": 1.0e-2,        // Maximum step-length
    
    "EPSF": 1.0e-4         // Step-length for finite difference determination of derivatives
}
