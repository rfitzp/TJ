{
    "epsa":   0.25,   // Inverse aspect-ratio of plasma boundar
    "q0":     0.14,   // Central safety-factor
    "beta0":  2.0e-2, // Central plasma beta
    "alphas": 4.0,    // Parallel current profile parameter
    "nus":    1.7,    // Parallel current profile parameter
    "alphap": 2.0,    // Pressure profile parameter
    "nup":    2.0,    // Pressure profile parameter

    "bwall":  1.2,   // Relative wall radius
    "dwall":  0.1,    // Relative wall thickness
    
    "mpol":  0,       // Poloidal mode number
    "ntor":  1,       // Toroidal mode number

    "kmax":  5.0,     // Maximum value of ntor*epsa in toroidal mode number scan
    "bmax":  1.5,     // Maximum value of bwall in wall position scan
    "Nscan": 100,     // Number of points in scans
    "betm":  1.2e-1,  // Maximum value of beta0 in beta scan
    "mmax":  20,      // Maximum value of mpol in scan
  
    "Ngrid": 5000,    // Number of radial grid points
    "eps":   1.0e-4,  // Closest approach to magnetic axis
    "delta": 1.0e-7,  // Closest approach to rational surface

    "acc":  1.0e-14,  // Adaptive integration accuracy
    "h0":   1.0e-4,   // Initial integration step-length 
    "hmin": 1.0e-10,  // Minimum integration step-length 
    "hmax": 1.0e-2,   // Maximum integration step-length

    "Eta":     1.0e-12, // Minimum magnitude of F at root F(x) = 0
    "Maxiter": 60,      // Maximum number of root finding iterations
    "Nint":    1000     // Number of root finding search intervals
    
