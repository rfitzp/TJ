{
    "qc":   1.1,   // Safety-factor on magnetic axis
    "qa":   4.5,   // Safety-factor at plasma boundary
    "pc":   0.0,   // Normalized plasma pressure on magnetic axis 
    "mu":   3.0,   // Pressure peaking parameter
    "epsa": 0.1,   // Inverse aspect-ratio of plasma

    "Hna": [0.0, 0.0, 0.0],   // Horizontal shaping harmonic values at plasma boundary
    "Vna": [0.0, 0.0, 0.0],   // Vertical shaping harmonic values at plasma boundary

    "bw": 1.2,     // Relative wall radius
   
    "wcoil": [ 0.7,  0.85, -0.85, -0.7],  // omega/M_PI coodinates of RMP coil strands
    "Icoil": [-1.0,  1.0,  -1.0,   1.0],  // Toroidal currents flowing in RMP coil strands

    "FLUX":  0,     // Flag for calculation of magnetic flux surfaces for visulalization purposes

    "eps": 1.0e-4,  // Distance of closest approach to magnetic axis
    "Ns":  20,      // Number of shaping harmonics included in calculation
    "Nr":  1000,    // Number of radial grid-points for calculation purposes
    "Nf":  500,     // Number of radial grid-points for visualization purposes
    "Nw":  1000,    // Number of angular grid-points for visualization purposes
    
    "acc":  1.0e-12,  // Integration accuracy
    "h0":   1.0e-4,   // Initial integration step-length
    "hmin": 1.0e-10,  // Minimum integration step-length
    "hmax": 1.0e-3,   // Maximum integration step-length 

    "EFIT":  0,       // Flag for calculating EFIT data
    "NRBOX": 1025,    // Number of R gridpoints in EFIT file
    "NZBOX": 1025,    // Number of Z gridpoints in EFIT file
    "rc":    1.1      // EFIT flux-surfaces calculated accurately up to r = rc (r = 1 is boundary)
}
